          "description" : "Number of transactions on one page"
        }, {
          "name" : "PageNumber",
          "in" : "query",
          "required" : false,
          "type" : "integer",
          "format" : "int32"
        }, {
          "name" : "SortOrder",
          "in" : "query",
          "required" : false,
          "type" : "string",
          "description" : "\"Ascending\", \"Descending\""
        }, {
          "name" : "DateFrom",
          "in" : "query",
          "required" : false,
          "type" : "string",
          "format" : "date",
          "description" : "Filter for start date (included). ISO 8601: www.iso.org/iso/home/standards/iso8601.htm"
        }, {
          "name" : "DateTo",
          "in" : "query",
          "required" : false,
          "type" : "string",
          "format" : "date",
          "description" : "Filter for end date (included). ISO 8601: www.iso.org/iso/home/standards/iso8601.htm"
        }, {
          "name" : "AmountMinimum",
          "in" : "query",
          "required" : false,
          "type" : "number",
          "description" : "Filter for minimum amount (included)"
        }, {
          "name" : "AmountMaximum",
          "in" : "query",
          "required" : false,
          "type" : "string",
          "description" : "Filter for maximum amount (included)"
        }, {
          "name" : "MCC",
          "in" : "query",
          "required" : false,
